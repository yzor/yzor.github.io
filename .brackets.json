{
    "sass.enabled": false,
    "path": {
        "*.scss": {
            "sass.enabled": true,
            "sass.options": {
                "outputDir": "../css/",
                "sourceComments": false,
                "outputStyle": "compressed"
            },
            "brackets-git.gitPath": "git",
            "livedev.multibrowser": true,
            "autoSavePrefs.timePrefs": 400,
            "autoSavePrefs.checkedPrefs": true,
            "linting.collapsed": false,
            "fonts.fontSize": "12px"
        }
    }
}
